public class Square implements Shape{

    private double lado;

    @Override
    public double getArea() {
        return lado*lado;
    }

    @Override
    public double getPerimeter() {
        return lado*4;
    }

    public double getLado() {
        return lado;
    }

    public void setLado(double lado) {
        this.lado = lado;
    }
}
